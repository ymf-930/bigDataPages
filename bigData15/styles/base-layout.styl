.page-content {
  height: 100%;
  width 100%;
  background-image: url("../assets/images/page-bg.jpg");
  background-repeat: no-repeat;
  background-position: center;
  background-size: 100% 100%;
  position relative

  .header {
    width: 100%;
    height: 100px;
    background-image: url("../assets/images/head-bg.png");
    background-size: 100% 100%;
    background-repeat: no-repeat;
    position: relative;

    .header-box {
      width 1158px;
    }

    .title {
      font-family: YouSheBiaoTiHei,sans-serif;
      font-size: 32px;
      letter-spacing: 4px;
      height 48px;
      line-height: 150%;
      text-align: center;
      color #fff;
      margin-top: 10px;
    }
  }


  .content-box {
    height: calc(100% - 100px)

    .center {
      width: 100%;
      padding 0 20px;
      margin-top: -40px;

      .center-left {
        width: 24%;
      }

      .center-center {
        width: 52%;
      }

      .center-right {
        width: 24%;
      }
    }
  }

}
