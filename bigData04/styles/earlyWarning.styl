    @extend .cir-1
    width: 100px
    height: 100px
    border: 1px dashed rgba(60, 150, 224, 0.5)
    transform translate(-50%, 42%) scale(7) rotateX(70deg)
    animation rotate-cir-6 infinite 120s

  .cir-7
    @extend .cir-1
    background-image: url(../images/yuan01.png)
    width: 50px
    height: 50px
    -webkit-background-size: cover
    background-size: cover
    background-repeat: no-repeat
    animation rotate-cir-7 infinite 120s
    border-color transparent

  .cir-8
    @extend .cir-1
    width: 50px
    height: 50px
    background-image: url(../images/yuan02.png)
    background-repeat: no-repeat
    border-color transparent
    animation rotate-cir-7 infinite 120s

@keyframes rotate-cir-5
  0% {
    transform: translate(-50%, 132%) scale(12) rotateX(70deg) rotateZ(0)
  }
  50% {
    transform: translate(-50%, 132%) scale(12) rotateX(70deg) rotateZ(0.5turn)
  }
  100% {
    transform: translate(-50%, 132%) scale(12) rotateX(70deg) rotateZ(1turn)
  }

@keyframes rotate-cir-6
  0% {
    transform: translate(-50%, 42%) scale(7) rotateX(70deg) rotateZ(0)
  }
  50% {
    transform: translate(-50%, 42%) scale(7) rotateX(70deg) rotateZ(-0.5turn)
  }
  100% {
    transform: translate(-50%, 42%) scale(7) rotateX(70deg) rotateZ(-1turn)
  }

@keyframes rotate-cir-7
  0% {
    transform: translate(-50%, 140%) scale(18) rotateX(70deg) rotateZ(0)
  }
  50% {
    transform: translate(-50%, 140%) scale(18) rotateX(70deg) rotateZ(-0.5turn)
  }
  100% {
    transform: translate(-50%, 140%) scale(18) rotateX(70deg) rotateZ(-1turn)
  }

@keyframes rotate3d {
  0% {
    transform: translate(-50%, -50%) rotateX(65deg) rotateZ(0) scale(1.4)
  }
  100% {
    transform: translate(-50%, -50%) rotateX(65deg) rotateZ(1turn) scale(1.4)
  }
}

@keyframes rotate3dOpposite {
  0% {
    transform: rotateZ(0)
  }
  100% {
    transform: rotateZ(-1turn)
  }
}

