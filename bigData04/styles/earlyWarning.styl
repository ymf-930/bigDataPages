//专项预警水球旋转
//$p = 156
$p = 140
$itemW = 134
$itemH = 134
$rotateW = 4 * $p
$rotateH = 3.46 * $p
.rotate
  position absolute
  margin 0 auto
  top 50%
  left 50%
  width ($rotateW) px
  height: ($rotateH) px
  transform-style preserve-3d
  // transform rotateX(-10deg)
  // background-color: #fff
  animation rotate3d linear 30s infinite
  z-index 92

  &:hover
    animation-play-state: paused;
    -webkit-animation-play-state: paused;

    .rotate-inner
      .rotate-inner-item
        animation-play-state: paused;
        -webkit-animation-play-state: paused;

  .rotate-inner
    position relative
    width: 100%
    height: 100%
    color #fff

    .rotate-inner-item
      animation rotate3dOpposite linear infinite 30s
      text-align center
      width: ($itemW) px
      height: ($itemH) px
      position relative

      &:nth-child(1)
        position absolute
        top: (0 - $itemH/ 2) px //-75px
        left: ($p - $itemW/ 2) px

      //81

      &:nth-child(2)
        position absolute
        top: (0 - $itemH/ 2) px //-75
        left: ($p* 3 - $itemW/ 2) px

      //393

      &:nth-child(3)
        position absolute
        top: ($rotateH/ 2 - $itemH/ 2) px //464.76
        left: ($p* 4 - $itemW/ 2) px

      //549

      &:nth-child(4)
        position absolute
        top: ($rotateH - $itemH/ 2) px //464.76
        left: ($p* 3 - $itemW/ 2) px

      //393

      &:nth-child(5)
        position absolute
        top: ($rotateH - $itemH/ 2) px //464.76
        left: ($p - $itemW/ 2) px

      //81

      &:nth-child(6)
        position absolute
        top: ($rotateH/ 2 - $itemH/ 2) px //194.88
        left: (0 - $itemW/ 2) px


      .cir-bg
        position absolute
        left: 50%
        top: 50%
        width: 90px
        height: 90px
        text-align center
        transform translate(-50%, -50%) rotateX(70deg) scaleY(7)
        //background-image: url('../images/ankang/tu6.png')
        background-position: center bottom
        -webkit-background-size: 100% 100%
        background-size: 100% 100%
        background-repeat: no-repeat

        .inner-item-text
          position absolute
          top 28px
          width: 100%
          //padding-top: 30px

          p
            font-size: 16px
            text-align center

          .text-title
            font-size: 10px
            text-align center
            width: 100%

          .text-count
            font-weight: 700
            font-size: 10px

            .unit-font
              font-size: 12px

          .text-percent
            position absolute
            bottom: -40px
            left: 35px
            font-weight: 700
            color #fbc606


